    "lint": "tsc && eslint ./src/**/*"
  },
  "devDependencies": {
    "globals": "15.3.0",
    "eslint": "9.2.0",
    "pie-math": "*",
    "tsup": "8.0.2",
    "typescript": "5.3.3",
    "typescript-eslint": "8.0.0-alpha.10"
  },
  "author": "Serge Iyashenko <serj.ilyashenko@gmail.com>",
  "keywords": [
    "svg",
    "svg chart",
    "chart",
    "diagram",
    "pie",
    "pie-chart",
    "doughnut",
    "doughnut chart",
