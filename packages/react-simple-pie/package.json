  },
  "devDependencies": {
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "pie-math": "*",
    "tsup": "8.0.2",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-dom": ">=15"
  },
  "author": "Serge Ilyashenko <serj.ilyashenko@gmail.com>",
  "keywords": [
    "svg",
    "diagram",
    "pie",
    "pie-chart"
