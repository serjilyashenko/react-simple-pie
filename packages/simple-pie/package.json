{
  "name": "simple-pie",
  "version": "1.3.6",
  "private": false,
  "description": "Super light weight and super simple svg pie diagram",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run build -- --watch",
